{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bensampo/laravel-enum": "^3.3",
        "encore/laravel-admin": "^1.8",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/cloud-secret-manager": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "khanamiryan/qrcode-detector-decoder": "^1.0",
        "laravel-admin-ext/log-viewer": "^1.0",
        "laravel-admin-ext/scheduling": "^1.1",
        "laravel-lang/lang": "~8.0",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "phpoffice/phpspreadsheet": "^1.17",
        "pragmarx/google2fa-laravel": "^1.4",
        "pragmarx/recovery": "^0.2.0",
        "predis/predis": "^1.1",
        "rc1021/binance-api": "*",
        "spatie/laravel-backup": "^6.15"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "matthiasmullie/minify": "^1.3",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "helpers/string_helper.php",
            "helpers/float_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./extra-pkgs/rc1021/binance-api"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:rc1021/php-binance-api.git"
        }
    ]
}
